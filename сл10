dic5 = {'a': 3, 'b': 4, 'c': 5, 'd': 6, 'e': 7, 'f': 8, 'g': 9}
dic6 = {'e': 20, 'f': 21, 'g': 22, 'h': 23, 'i': 24, 'j': 25, 'k': 26, 'l': 27}
dic7 = {}
dic8 = dic5.copy()
for k in dic6.keys():
    if k not in dic5.keys():
        dic8[k] = dic6[k]
print(dic8)
print("Новый словарь на основе dic6 = ",dic7)
print(dic8)