def to_str(num):
  x = str(num)
  dict_second = {
      '0' : ' Ноль',
      '1' : ' Один',
      '2' : ' Два',
      '3' : ' Три',
      '4' : ' Четыре',
      '5' : ' Пять',
      '6' : ' Шесть',
      '7' : ' Семь',
      '8' : ' Восемь',
      '9' : ' Девять'
  }
  dict_first = {
      '2' : 'Двадцать',
      '3' : 'Тридцать',
      '4' : 'Сорок',
      '5' : 'Пятьдесят',
      '6' : 'Шестьдесят',
      '7' : 'Семьдесят',
      '8' : 'Восемьдесят',
      '9' : 'Девяносто'
  }
  dict_surprise = {
      '10' : 'Десять',
      '11' : 'Одиннадцать',
      '12' : 'Двенадцать',
      '13' : 'Тринадцать',
      '14' : 'Четырнадцать',
      '15' : 'Пятнадцать',
      '16' : 'Шестнадцать',
      '17' : 'Семнадцать',
      '18' : 'Восемнадцать',
      '19' : 'Девятнадцать'
  }
  if (num < 10):
    return dict_second[x]
  if ((num >= 10) and (num < 20)):
    return dict_surprise[x]
  if ((num >= 20) and (num < 100)):
    if (num % 10 == 0): return dict_first[x[0]]
    else: return dict_first[x[0]] + dict_second[x[1]]
  else:
    return 'error'
print(to_str(40))